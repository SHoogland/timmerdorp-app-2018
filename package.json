		"cordova-plugin-whitelist": "^1.3.4",
		"ionic-angular": "3.9.10",
		"ionicons": "^4.6.3",
		"list": "2.0.19",
		"parse": "^2.13.0",
		"phonegap-plugin-barcodescanner": "^8.1.0",
		"rxjs": "^6.5.5",
		"rxjs-compat": "^6.5.5",
